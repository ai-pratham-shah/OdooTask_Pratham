<odoo>
    <!-- This form is created for move one field to another page called book details -->
    <record id="view_product_template_barcode_inherit_form" model="ir.ui.view">
        <field name="name">product.template.inherit.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="ak_library_management.view_product_template_inherit_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='publisher']" position="after">
                <field name="barcode"  position="move"/>
            </xpath>
        </field>
    </record>
</odoo>